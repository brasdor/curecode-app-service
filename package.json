{
  "name": "curecode-app-service",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "^22",
    "npm": "^10"
  },
  "scripts": {
    "comment": "Windows users must use cross-env syntax for environment variables. :win",
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "studio": "prisma studio",
    "start": "npm run db:migrations:run && NODE_ENV=development nest start --env-file-if-exists=.env",
    "start:dev": "npm run db:migrations:run && cross-env NODE_ENV=development nest start --watch --env-file-if-exists=.env",
    "start:dev:linux": "npm run db:migrations:run && NODE_ENV=development nest start --watch --env-file-if-exists=.env",
    "start:debug": "npm run db:migrations:run && NODE_ENV=development nest start --debug --watch --env-file-if-exists=.env",
    "start:prod": "node dist/src/main",
    "db:migrations:create": "DATABASE_URL=\"postgresql://postgres:postgres@localhost:5432/curecode_app_service_db?schema=curecode_app_service\" prisma migrate dev --create-only",
    "db:migrations:run": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/curecode_app_service?schema=curecode_app_service prisma migrate deploy",
    "db:migrations:run:linux": "DATABASE_URL=\"postgresql://postgres:postgres@localhost:5432/curecode_app_service?schema=curecode_app_service\" prisma migrate deploy",
    "db:migrate": "npm run db:migrations:create && npm run db:migrations:run && prisma generate",
    "db:restart": "docker compose down --volumes && docker compose up --wait && npm run db:migrations:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "openapi-ts": "openapi-ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "0.0.64",
    "@hey-api/client-axios": "^0.2.9",
    "@hey-api/client-fetch": "^0.4.2",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "10.4.4",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "10.2.3",
    "@prisma/adapter-pg": "5.22.0",
    "@prisma/client": "5.22.0",
    "ai": "3.4.9",
    "base64-arraybuffer": "^1.0.2",
    "bcrypt": "5.1.1",
    "body-parser": "^1.20.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "date-fns": "4.1.0",
    "envalid": "8.0.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.67.3",
    "openapi": "1.0.1",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pg": "8.16.3",
    "puppeteer": "23.5.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "7.8.1",
    "swagger-ui-express": "5.0.1",
    "ts-pattern": "5.5.0",
    "util": "^0.12.5",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.54.0",
    "@nestjs/cli": "10.4.5",
    "@nestjs/schematics": "10.1.4",
    "@nestjs/testing": "10.4.4",
    "@types/bcrypt": "5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.13",
    "@types/node": "22.7.5",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.8.1",
    "@typescript-eslint/parser": "8.8.1",
    "cross-env": "^10.1.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.12.0",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "prisma": "5.22.0",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
